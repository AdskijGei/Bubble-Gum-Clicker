local Section = Tab:NewSection("Random Staff")
Section:NewButton("üíÄ", "üíÄ", function()
for i=1,1000000000000000000000000000000000000000000 do
    while task.wait(0.00000000000000000000000000000000000000001) do
local args = {
    [1] = {
        [1] = {
            [1] = "Spawn World",
            [2] = "Heaven"
        },
        [2] = {
